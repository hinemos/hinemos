                              slapmBaseGroup2,
                              slapmNotGroup2
                           }
          GROUP slapmEndSystemGroup2
          GROUP slapmEndSystemNotGroup2
          GROUP slapmBaseGroup
          GROUP slapmNotGroup
          GROUP slapmOptionalGroup





          GROUP slapmEndSystemGroup
          GROUP slapmEndSystemNotGroup
      ::= { slapmCompliances 1 }


